//for loop example

void main(){
  
  for(int i=1; i<=5; i++){
    print(1);
  }
}
OP:-1
1
1
1
1
====================================================
//for in loop
void main(){
  
  List<int> l =[10,20,30,40,50];
  for (var x in 1){
    print(x);
  }
}

=================================================
//for each loop example
void main(){
  
  List<int> l =[10,20,30,40,50];
  
  l.forEach(show);
}

void show(var x){
  print(x);
}
OP:-10
20
30
40
50
=================================================
//for each loop example
void main(){
  
  List<int> l =[10,20,30,40,50];
  
  l.forEach((var x)
  {
   print(x);
  });
}
OP:- 10
